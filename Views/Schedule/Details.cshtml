@model AspnetCoreMvcFull.Models.Schedule

<div>
	<h4>Schedule Details</h4>
	<hr />
	<dl class="row">
		<dt class="col-sm-2">Truck ID</dt>
		<dd class="col-sm-10">@Model.TruckId</dd>

		<dt class="col-sm-2">Collector</dt>
		<dd class="col-sm-10">@Model.Collector?.FirstName @Model.Collector?.LastName</dd>

		<dt class="col-sm-2">Route</dt>
		<dd class="col-sm-10">@Model.Road?.Name</dd>

		<dt class="col-sm-2">Scheduled Start</dt>
		<dd class="col-sm-10">@Model.ScheduleStartTime.ToString("g")</dd>

		<dt class="col-sm-2">Scheduled End</dt>
		<dd class="col-sm-10">@Model.ScheduleEndTime.ToString("g")</dd>

		<dt class="col-sm-2">Actual Start</dt>
		@* <dd class="col-sm-10">@Model.ActualStartTime.ToString("g")</dd> *@

		<dt class="col-sm-2">Actual End</dt>
		@* <dd class="col-sm-10">@Model.ActualEndTime.ToString("g")</dd> *@

		<dt class="col-sm-2">Status</dt>
		<dd class="col-sm-10">@Model.Status</dd>

		<dt class="col-sm-2">Admin Notes</dt>
		<dd class="col-sm-10">@Model.AdminNotes</dd>
	</dl>
</div>
<div>
	<a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
	<a asp-action="Index">Back to List</a>
</div>
